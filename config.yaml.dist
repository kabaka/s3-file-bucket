# Your AWS remote access credentials

:AWS:
  :access_key_id: ''
  :secret_access_key: ''

# S3 bucket name (must already exist)

:bucket: ''

# Prefix into which files should go.

:prefix: ''

# Days after which files should expire. Changes to this setting are
# retroactive, effective on the next upload.

:expiration: 30

# If true, store files with reduced redundancy in S3.

:reduced_redundancy: false
